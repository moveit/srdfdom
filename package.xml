<package format="2">
  <name>srdfdom</name>
  <version>0.6.4</version>
  <description>Parser for Semantic Robot Description Format (SRDF).</description>

  <author email="isucan@willowgarage.com">Ioan Sucan</author>
  <author email="gwalck@techfak.uni-bielefeld.de">Guillaume Walck</author>
  <maintainer email="moveit_releasers@googlegroups.com">MoveIt Release Team</maintainer>

  <license>BSD</license>
  <url type="website">http://ros.org/wiki/srdfdom</url>
  <url type="bugtracker">https://github.com/ros-planning/srdfdom/issues</url>
  <url type="repository">https://github.com/ros-planning/srdfdom</url>

